<div class="modal fade" id="withdrawApplicationModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
  aria-labelledby="withdrawApplicationModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5" id="withdrawApplicationModalLabel">Withdraw Application</h1>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <div class="row mb-3">
          <div class="col-sm-12">
            <p class="lead">Are you sure you want to withdraw this application?</p>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary whl-action-cancel-withdraw-application" data-bs-dismiss="modal">Cancel</button>
        <button type="button" class="btn btn-primary whl-action-confirm-withdraw-application" data-applicationid="@Model.ApplicationId">Withdraw</button>
        <label class="whl-label-confirm-withdraw-application-inprogress">Withdrawing&hellip;</label>
      </div>
    </div>
  </div>
</div>